
<h1>All Teams</h1>
<ul>
  <% teams.forEach(function(team) { %>
    <li>
      <%= team.name %>
      <form method="POST" action="/teams/<%= team._id %>?_method=DELETE" style="display:inline;">
        <button type="submit">Delete</button>
      </form>
      <a href="/teams/<%= team._id %>">View Athletes</a>
    </li>
  <% }); %>
</ul>

<a href="/">Back to home</a>

