<h1>Athletes</h1>
<ul>
  <% athletes.forEach(athlete => { %>
    <li>
      <p><%= athlete.name %></p>

      <!-- Edit -->

      
      <form action="/athletes/<%= athlete._id %>/edit" method="GET" style="display:inline;">
        <button type="submit">Edit</button>
      </form>

      <!-- Delete -->


      <form action="/athletes/<%= athlete._id %>?_method=DELETE" method="POST" style="display:inline;">
        <button type="submit">Delete</button>
      </form>
    </li>
  <% }); %>
</ul>

<a href="/">Back to home</a>